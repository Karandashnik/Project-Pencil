<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>User Dashboard</title>
   <meta name="viewport" content="width=device-width, initial scale=1.0">
   <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
  <script type='text/javascript' src='https://cdnjs.cloudflare.com/ajax/libs/backbone.js/1.2.3/backbone-min.js'></script>
  <link rel="stylesheet" href="css/calendarStyle.css" media="screen">
</head>
<body>
<div class="container">  
<div class="col-md-6">
<div id="calendar">

  <div class="calHeader">
    <span class="left button" id="prev">&lang;</span>
    <span class="month-year" id="label"> October 2015 </span>
    <span class="right button" id="next"> &rang; </span>
  </div>
  <table id ="weekdays"></table>
  <div id="calGrid">
    <table class="curr" id="numDays">
    </table>
 </div>
</div>
</div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script src="js/blankCalendar.js"></script>
  <script> 
  var calendar = Calendar();
  calendar.init();
  calendar.createDays();
  calendar.dayNums();
  </script>

</body>
</html>
